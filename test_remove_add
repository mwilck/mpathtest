# -*- mode: shell-script -*-
test_remove_add() {
    local wait=$1

    for path in ${PATHS[@]}; do
	action remove $path
	usleep ${wait:-1000}
    done

    sleep 2 removed paths
    
    new_step
    
    for path in ${PATHS[@]}; do
	action add $path
	usleep ${wait:-1000}
    done
    sleep 2 added paths

    new_step -k

}
